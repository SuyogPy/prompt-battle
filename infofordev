the scheme used is  -- Enable UUID extension
create extension if not exists "uuid-ossp";

-- IMAGE ROUND TABLE
create table image_round (
    id uuid primary key default uuid_generate_v4(),
    name text not null,
    prompt text not null,
    image_path text not null,
    score integer,
    created_at timestamp default now()
);

-- TEXT ROUND TABLE
create table text_round (
    id uuid primary key default uuid_generate_v4(),
    name text not null,
    prompt text not null,
    response text not null,
    score integer,
    created_at timestamp default now()
);

-- Index for faster judge loading
create index idx_image_created_at on image_round(created_at);
create index idx_text_created_at on text_round(created_at);




